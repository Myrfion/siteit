        "highlight.js": "^11.6.0",
        "remarkable": "^2.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@babel/core": "^7.19.3",
        "@babel/node": "^7.19.1",
        "@babel/plugin-syntax-import-assertions": "^7.18.6",
        "@babel/preset-env": "^7.19.4",
        "babel-plugin-import": "^1.13.5",
        "prettier": "2.7.1"
    }
}
